msgid "Icon"
msgstr "Symbol"

#. settings-schema.json->notification-enabled->description
msgid "Notification"
msgstr "Benachrichtigung"

#. settings-schema.json->notification-enabled->tooltip
msgid "Show notifications"
msgstr "Zeige Benachrichtigung"

#. settings-schema.json->terminal-app->description
msgid "Terminal application"
msgstr "Terminalanwendung"

#. settings-schema.json->terminal-app->tooltip
msgid "Set your default Terminal application."
msgstr "Stellen Sie Ihre Standardterminalanwendung ein."

#. settings-schema.json->terminal-exec-flag->description
msgid "Terminal execution flag"
msgstr "Terminal Ausführungs-Flag"

#. settings-schema.json->terminal-exec-flag->tooltip
msgid "Set the terminal execution flag to run the ssh command."
msgstr "Stellen Sie die Terminal Ausführung-Flag ein, um den Befehl SSH auszuführen."

#. settings-schema.json->custom-title->description
msgid "Custom window title"
msgstr "Benutzerdefinierte Fenstertitel"

#. settings-schema.json->custom-title->tooltip
msgid "Set if the window should be titled with the connection name."
msgstr "Soll das Fenster mit dem Verbindungsnamen betitelt werden."

#. settings-schema.json->custom-title-flag->description
msgid "Custom window title flag"
msgstr "Benutzerdefinierte Fenstertitel-Flag"

#. settings-schema.json->custom-title-flag->tooltip
msgid "Set the terminal flag to use a custom window title."
msgstr "Stellen Sie die Terminal-Flag ein, um einen benutzerdefinierten Fenstertitel zu verwenden."

#. settings-schema.json->custom-profile->description
msgid "Custom terminal profile"
msgstr "Benutzerdefinierte Benutzerprofil"

#. settings-schema.json->custom-profile->tooltip
msgid "Set if the terminal should be use a specific profile, defined in connections."
msgstr "Soll das Terminal ein bestimmtes Profil verwenden, das in Verbindungen definiert ist."

#. settings-schema.json->custom-profile-flag->description
msgid "Custom terminal profile flag"
msgstr "Benutzerdefinierte Benutzerprofil-Flag"

#. settings-schema.json->custom-profile-flag->tooltip
msgid "Set the terminal flag to use a custom profile."
msgstr "Stellen Sie die Terminal-Flag ein, um einen benutzerdefiniertes Benutzerprofil zu verwenden."
